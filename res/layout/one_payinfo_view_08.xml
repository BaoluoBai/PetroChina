<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="@drawable/withhold_info">

    <TextView
        android:id="@+id/tv_paymoney_one"
        android:layout_width="200px"
        android:layout_height="60px"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:layout_marginRight="90dp"
        android:layout_marginTop="135dp"
        android:text="1234.56"
        android:textColor="#FFFFFF"
        android:textSize="45sp" />

    <TextView
        android:id="@+id/tv_yue_one"
        android:layout_width="200px"
        android:layout_height="60px"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:layout_marginRight="90dp"
        android:layout_marginTop="240dp"
        android:text="6666.56"
        android:textColor="#FFFFFF"
        android:textSize="45sp" />

    <Button
        android:id="@+id/btn_printyes_one"
        android:layout_width="170dp"
        android:layout_height="84dp"
        android:layout_marginTop="565dp"
        android:layout_marginLeft="75dp"
        style="@null"
        android:background="#00000000"/>

    <Button
        android:id="@+id/btn_printno_one"
        android:layout_width="170dp"
        android:layout_height="84dp"
        android:layout_alignBaseline="@+id/btn_printyes_one"
        android:layout_alignBottom="@+id/btn_printyes_one"
        android:layout_marginLeft="296dp" 
        style="@null"
        android:background="#00000000"/>
    
    <ImageView 
        android:id="@+id/iv_float_one"
        android:layout_height="500px"
        android:layout_width="match_parent"
        android:src="@drawable/bg_float"
        android:layout_alignParentBottom="true"
        android:visibility="gone"/>

</RelativeLayout>
